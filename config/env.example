# 에브리타임 로그인 정보
# 실제 값으로 변경하여 사용하세요
EVERYTIME_ID=your_evertime_id
EVERYTIME_PASSWORD=your_evertime_password

# 크롤링 설정
HEADLESS_MODE=false
DELAY_BETWEEN_REQUESTS=2.0

# 로그 레벨
LOG_LEVEL=INFO

# MongoDB
MONGO_INITDB_ROOT_USERNAME=root
MONGO_INITDB_ROOT_PASSWORD=example
MONGO_DB_NAME=crawller
MONGO_URI=mongodb://root:example@localhost:27017/crawller?authSource=admin

# Pinecone 연동
PINECONE_API_KEY=your_api_key_here
PINECONE_INDEX=courses-dev

# LLM API 설정
# OpenAI 또는 Gemini 중 하나를 사용
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
LLM_PROVIDER=gemini  # gemini 또는 openai
