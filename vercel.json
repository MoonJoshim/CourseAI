{
    "version": 2,
    "builds": [
      {
        "src": "frontend/react-app/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "installCommand": "npm install --prefix frontend/react-app || npm install",
    "buildCommand": "npm run build --prefix frontend/react-app || npm run build",
    "framework": "create-react-app"
  }